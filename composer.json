{
    "name": "aoe/typo3-cms-extension-restler",
    "require": {
        "luracast/restler": "3.*@dev"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Luracast/Restler.git"
        }        
    ],
    "autoload": {
        "psr-0": {
            "Luracast\\Restler\\": "vendor/luracast/restler/vendor/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-feature/composer": "1.0.x-dev"
        }
    }    
}

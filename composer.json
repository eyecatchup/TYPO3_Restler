{
    "name": "aoe/typo3-cms-extension-restler",
    "require": {
        "luracast/restler": "dev-v3"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "luracast/restler",
                "version": "dev-v3",
                "source": {
                    "url": "https://github.com/AOEpeople/Restler.git",
                    "type": "git",
                    "reference": "origin/v3"
                }
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "Luracast\\Restler\\": "vendor/luracast/restler/vendor/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-feature/composer": "3.0.x-dev"
        }
    }    
}
